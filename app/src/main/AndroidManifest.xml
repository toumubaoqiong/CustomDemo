<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.vince.demo.customdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <!-- 在SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!-- 访问internet权限 -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <!-- 访问Camera权限 -->
    <uses-permission android:name="android.permission.CAMERA"/>

    <uses-feature android:name="android.hardware.camera"/>

    <uses-permission android:name="com.bbk.studyos.launcher.permission.INSERT_WIDGET_PAGE"/>
    <uses-permission android:name="com.bbk.studyos.launcher.permission.READ_SETTINGS"/>
    <uses-permission android:name="com.bbk.studyos.launcher.permission.WRITE_SETTINGS"/>
    <!-- 开机广播 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>

    <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

    <!-- 往SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <!-- 允许应用程序使用SYSTEM_ALERT_TYPE类型在所有其他应用程序之上显示窗口很少有程序用到这个许可；这些窗口与用户进行系统级的交互 -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>

    <!-- 允许应用打开网络套接口 -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <!-- 允许程序访问Wi-Fi网络状态信息 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <!-- 允许应用改变Wi-Fi的连接状态 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>

    <!-- 唤醒/待机权限 -->
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <!-- 账户系统权限 -->
    <uses-permission android:name="com.eebbk.bfc.permission.ACCOUNTMANAGER"/>

    <!-- 文件上传权限 -->
    <uses-permission android:name="com.eebbk.bfc.permission.BBK_UPLOAD_WITHOUT_NOTIFICATION"/>

    <!-- 允许程序后台下载，不发送通知栏消息 -->
    <uses-permission android:name="android.permission.DOWNLOAD_WITHOUT_NOTIFICATION"/>

    <!-- 文件下载权限 -->
    <uses-permission android:name="com.eebbk.bfc.permission.BBK_DOWNLOAD_WITHOUT_NOTIFICATION"/>

    <!-- 下载结束回调权限 -->
    <uses-permission android:name="com.eebbk.bfc.SEND_BBK_DOWNLOAD_COMPLETED_INTENTS"/>

    <!-- 获取网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>

    <!-- 最新DA权限 -->
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>

    <!-- 截屏所需要的权限 -->
    <uses-permission android:name="android.permission.READ_FRAME_BUFFER"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <!-- 安装apk权限 -->
    <uses-permission android:name="android.permission.INSTALL_PACKAGES"/>

    <!-- 打开相机权限 -->
    <uses-permission android:name="android.permission.CAMERA"/>

    <!-- Upload System Start -->
    <!-- Allows to queue uploads without a notification shown while the upload runs. -->
    <permission
        android:name="com.eebbk.bfc.permission.BBK_UPLOAD_WITHOUT_NOTIFICATION"
        android:permissionGroup="android.permission-group.NETWORK"
        android:protectionLevel="normal"/>

    <!-- Allows advanced access to the Upload Manager -->
    <permission
        android:name="com.eebbk.bfc.permission.ACCESS_BBK_UPLOAD_MANAGER"
        android:protectionLevel="normal"/>
    <!-- custom -->
    <uses-permission android:name="com.eebbk.bfc.permission.ACCESS_BBK_UPLOAD_MANAGER"/>
    <uses-permission android:name="com.eebbk.bfc.permission.BBK_UPLOAD_WITHOUT_NOTIFICATION"/>

    <application
        android:name=".crash_handler.MyApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppThemeEx">
        <activity
            android:name=".demo_aidl.AidlActivity"
            android:label="@string/title_activity_aidl"/>

        <service
            android:name=".demo_aidl.ControlMusicService"
            android:process=":remote">
            <intent-filter>

                <!-- AIDL完整路径名。必须指明，客户端能够通过AIDL类名查找到它的实现类 -->
                <action android:name="com.vince.demo.customdemo.demo_aidl.ControlMusicService"/>
            </intent-filter>
        </service>

        <activity android:name=".marqueeTextView.MarqueeTextViewActivity">
        </activity>
        >
        <activity
            android:name=".loading.LoadingActivity"
            android:label="@string/title_activity_loading"/>
        <activity android:name=".loading.MainActivity"/>
        <activity
            android:name=".net_set.NetworkSettingActivity"
            android:label="@string/title_activity_network_setting"/>
        <activity
            android:name=".acquisition_configuration.AcquisitionConfiguration"
            android:label="@string/title_activity_acquisition_configuration"/>
        <activity
            android:name=".tooltip.TooltipActivity"
            android:label="@string/title_activity_tooltip"/>
        <activity android:name=".radiogroup.MainActivity"/>
        <activity
            android:name=".webview_html.WebViewActivity"
            android:configChanges="orientation|screenSize|keyboardHidden|keyboard|navigation|mnc|mcc"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>
        <activity android:name=".crash_handler.CrashReportActivity"/>
        <activity android:name=".version_update.VersionUpdateActivity"/>
        <activity
            android:name=".login.LoginActivity"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustResize"/>
        <activity
            android:name=".http.activity.GetAPictureFromInternetActivity"
            android:label="@string/app_name"/>
        <activity
            android:name=".http.activity.WebCodeViewerActivity"
            android:label="@string/app_name"/>
        <activity android:name=".http.activity.GetXMLActivity"/>
        <activity android:name=".http.activity.UploadUserInfoActivity"/>
        <activity android:name=".viewfipper.ViewFlipperActivity"/>
        <activity android:name=".viewpager.ViewPagerActivity"/>
        <activity android:name=".viewpager.ViewPagerOthersActivity"/>
        <activity android:name=".textureview.TextureViewActivity"/>
        <activity android:name=".controls.ExpandTextViewActivity"/>
        <activity android:name=".controls.ExpandEditTextActivity"/>
        <activity android:name=".custom_videoview.CustomVideoViewActivity"/>
        <activity android:name=".webview_html.TestActivity">
        </activity>
        <activity android:name=".skipotherapp.SkipOtherActivity">
        </activity>
        <activity android:name=".application.FirstMainActivity">
        </activity>
        <activity android:name=".application.SecondMainActivity">
        </activity>
        <activity android:name=".circlarLayout.CircularLayoutActivity">
        </activity>
        <activity android:name=".xfermode.XfermodeActivity">

        </activity>
        <activity android:name=".frmeanimation.FrameAnimationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>